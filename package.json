{
  "name": "vaporvee-web-next",
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.42",
  "private": true,
  "scripts": {
    "build": "turbo build",
